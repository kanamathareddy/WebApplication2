    @*https://www.jqueryscript.net/text/Animated-Text-Truncate-Content-Toggle-Plugin-jQuery-Collapser.html*@
    <script src="~/js/jquery.collapser.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#Faculty > blockquote').show();

            $('#currentpageBreadcrum').click(function () {
                $('[href=".tab-main"]').tab('show');
                $('#currentpageBreadcrum').html("Faculty");
                $('#breadcrumb-nav-section li').last().hide();
            });
            $('#Faculty > blockquote li, .tab-content .tab-main .href-links').click(function () {
                $('#currentpageBreadcrum').html("<a class=\"href-links\" href=\".tab-main\" data-toggle=\"tab\">Faculty</a>");
            });

            //Tag and the text should be at same level for this to work as expected.
            $(".textTruncation").collapser({
                target: 'next',
                mode: 'words',
                truncate: 20,
                speed: 'slow',
                showText: ' Show More',
                hideText: ' Hide Text',
                ellipsis: '...',
                showClass: 'show-class',
                hideClass: 'hide-class'
            });
        });
    </script>
}
